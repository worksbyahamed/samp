//------------------------------Teleport to Bank--------------------------------
CMD:telebank(playerid,params[]) {
	#pragma unused params
	if(AccInfo[playerid][Jailed] == 1) return Inter_SendClientMessage(playerid,COLOR_BRIGHTRED,"You cannot escape your punishment!");
	GetPlayerPos(playerid,Pos[playerid][0],Pos[playerid][1],Pos[playerid][2]); // save x,y and z for this player.
	GetPlayerFacingAngle(playerid,Pos[playerid][3]); // save the facing angle for this player.
	tinteriorid = GetPlayerInterior(playerid);
	TimedTele(playerid, "Welcome to the Bank!", -27.7789, -54.9291, 1003.5469, 6, GetPlayerVirtualWorld(playerid), 10);
	return 1;
}
CMD:exitbank(playerid,params[]) {
	#pragma unused params
	if(AccInfo[playerid][Jailed] == 1) return Inter_SendClientMessage(playerid,COLOR_BRIGHTRED,"You cannot escape your punishment!");
	if(CloseToBank(playerid) == 999) {
		Inter_SendClientMessage(playerid, COLOR_BRIGHTRED, "You must be in a 24/7 in order to exit the bank!");
		return 1;
	}
	Streamer_UpdateEx(playerid,Pos[playerid][0],Pos[playerid][1],Pos[playerid][2]);
	SetPlayerPos(playerid,Pos[playerid][0],Pos[playerid][1],Pos[playerid][2]); // set his saved position x,y and z.
	SetPlayerFacingAngle(playerid,Pos[playerid][3]); // set his saved facing angle.
	SetCameraBehindPlayer(playerid); // set the camera..behind the player (looks better).
	SetPlayerInterior(playerid,tinteriorid);
	return 1;
}
