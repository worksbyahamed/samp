	Inter_SendClientMessage(playerid, 0x00C7FFAA, string);
	AccInfo[playerid][weed] -= gramtogive;
	AccInfo[giveplayerid][weed] += gramtogive;
	SetPlayerScore(playerid, GetPlayerScore(playerid) +1);
	commitedcrimerecently[playerid] +=180;
	for(new i=0;i<GetMaxPlayers();i++)
	{
		if(LawEnforcementRadio[i] == 1) {
			new LuxZone[MAX_ZONE_NAME];
			GetPlayerInZone(i, LuxZone, MAX_ZONE_NAME);
			format(string, sizeof(string), "DISPATCH: (WEED DEALING) Suspected weed deal has taken place - Suspect: %s(%d) Location: %s",dealername,playerid,LuxZone);
