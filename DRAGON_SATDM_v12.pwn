		strmid(VehicleInfo[GetPlayerVehicleID(playerid)][owner],PlayerName2(playerid), 0, strlen(PlayerName2(playerid)), 255);
		VehicleInfo[GetPlayerVehicleID(playerid)][bought] = 1;
		if(AccInfo[playerid][vowner] == 0){
			AccInfo[playerid][vowner] = 1;
			AccInfo[playerid][vowned] = GetPlayerVehicleID(playerid);
			format(stringa, sizeof(stringa), "You just bought this %s for $%d. In Car Calling Slot #1", VehicleInfo[GetPlayerVehicleID(playerid)][vname], VehicleInfo[GetPlayerVehicleID(playerid)][vehiclecost],VehicleInfo[GetPlayerVehicleID(playerid)][vname]);
			Inter_SendClientMessage(playerid, COLOR_GREEN, stringa);
		}
		else if(AccInfo[playerid][vowner2] == 0){
