//------------------------------------Kill--------------------------------------
CMD:kill(playerid,params[]) {
	#pragma unused params
	if(IsSpawned[playerid] == 1)
	{
		if(cuffed[playerid] == 1)
		{
			Inter_SendClientMessage(playerid, COLOR_RED, "You are in Hand Cuffs and cannot kill yourself!");
			return 1;
		}
		if(IsPlayerInAnyVehicle(playerid)) {
			if(VehicleInfo[GetPlayerVehicleID(playerid)][Temp] == 1) SetTimerEx("EraseVeh", 5000,0,"i",GetPlayerVehicleID(playerid));
		}
		new Float:health;
		if(GetPlayerHealth(playerid, health) >0) {
			SetPlayerScore(playerid,GetPlayerScore(playerid)-1);
			SetPlayerHealth(playerid,0);
			return 1;
		}
	}
	return 1;
}
