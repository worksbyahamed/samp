//==========================
#if EnableCamHack == true
CMD:lcam(playerid,params[])
{
	#pragma unused params
	if(AccInfo[playerid][Level] >= 3)
	{
		if(AccInfo[playerid][Spawned] == 1)
	 	{
			if(AccInfo[playerid][InCamMod] == 0)
			{
	  			if(AccInfo[playerid][pGps] != -1)
				return Inter_SendClientMessage(playerid, red, "ERROR: First Disable the Gps System! (/gps off)");

				if(AccInfo[playerid][SpecType] != ADMIN_SPEC_TYPE_NONE)
				return Inter_SendClientMessage(playerid, red, "ERROR: First Disable the Spectating System! (/lspecoff)");

				Inter_SendClientMessage(playerid, 0x199B00AA, " ");
				Inter_SendClientMessage(playerid, 0x199B00AA, "|- LuX CamSystem - Commands & Controlls -| ");
				Inter_SendClientMessage(playerid, 0x47CCB7AA, "*/lcam: Start/Stop Flying Camera Mode");
				Inter_SendClientMessage(playerid, 0x47CCB7AA, "*/lockcam: Locks/Unlock the camera and player is free to move.");
				Inter_SendClientMessage(playerid, 0x47CCB7AA, "*/follow: Locks the camera and keeps looking at player.");
    			Inter_SendClientMessage(playerid, 0xFA8072AA, "*Arrow-keys*: Move camera forward/backward/left/right");
            	Inter_SendClientMessage(playerid, 0xFA8072AA, "*Walk-key + Arrow-keys*: *Move camera up/down");
            	Inter_SendClientMessage(playerid, 0xFA8072AA, "*Crouch-key + Arrow-keys*: *Rotate camera up/down/left/right");
            	Inter_SendClientMessage(playerid, 0xFA8072AA, "*Sprint-key*: Speeds up every movement");

				TogglePlayerControllable(playerid, 0);
				AccInfo[playerid][LockedCam] = 0;
				AccInfo[playerid][InCamMod] = 1;
				FollowOn[playerid] = 0;
				GetPlayerPos(playerid, PCP[playerid][0], PCP[playerid][1], PCP[playerid][2]);
				GetPlayerFacingAngle(playerid, PCA[playerid]);

				GameTextForPlayer(playerid,"~n~~n~~n~~n~~n~~n~~n~~w~~g~Cam System ~y~Enabled!",1000,3);

				if(IsPlayerInAnyVehicle(playerid))
				{
					GetVehicleZAngle(GetPlayerVehicleID(playerid), PCA[playerid]);
				}
				PCL[playerid][0] = PCP[playerid][0];
				PCL[playerid][1] = PCP[playerid][1];
				PCL[playerid][2] = PCP[playerid][2];
				PCP[playerid][0] = PCP[playerid][0] - (5.0 * floatsin(-PCA[playerid], degrees));
				PCP[playerid][1] = PCP[playerid][1] - (5.0 * floatcos(-PCA[playerid], degrees));
				PCP[playerid][2] = PCP[playerid][2]+2.0;
				SetPlayerCameraPos(playerid, PCP[playerid][0], PCP[playerid][1], PCP[playerid][2]);
				SetPlayerCameraLookAt(playerid, PCL[playerid][0], PCL[playerid][1], PCL[playerid][2]);
			 	KeyTimer[playerid] = SetTimerEx("CheckKeyPress", 50, 1, "i", playerid);
			}
			else
			{
				TogglePlayerControllable(playerid, 1);
				KillTimer(KeyTimer[playerid]);
				AccInfo[playerid][InCamMod] = 0;
				SetCameraBehindPlayer(playerid);
				GameTextForPlayer(playerid,"~n~~n~~n~~n~~n~~n~~n~~w~~g~Cam System ~r~Disabled!",1000,3);
			}
			return 1;
		}
		else return Inter_SendClientMessage(playerid, 0xFF0000AA, "ERROR: Please Spawn First!");
	}
	else return ErrorMessages(playerid, 1);
}
