CMD:countdown(playerid,params[])
{
    if(AccInfo[playerid][Level] >= 3)
	{
	    if(CdStated == 0)
	    {
	 		new Index;
		    new tmp[256];  tmp  = strtok(params,Index);
			new tmp2[256]; tmp2 = strtok(params,Index);

			if(isnull(tmp))
			return Inter_SendClientMessage(playerid, LIGHTBLUE2, "Usage: /countdown [Seconds] [Freeze 1/0]") &&
	   	    Inter_SendClientMessage(playerid, orange, "Function: Will create a CountDown for all Players! (1-Freeze, 0-NoFreeze)");

			cd_sec = strval(tmp);
			if(cd_sec < 1 || cd_sec > 1000)
			return Inter_SendClientMessage(playerid,COLOR_WHITE,"ERROR: Seconds between 1-1000");

			if(isnull(tmp2))
			return Inter_SendClientMessage(playerid, LIGHTBLUE2, "Usage: /countdown [Seconds] [Freeze 1/0]") &&
	   	    Inter_SendClientMessage(playerid, orange, "Function: Will create a CountDown for all Players! (1-Freeze, 0-NoFreeze)");

			cd_f = strval(tmp2);
			if(cd_f < 0 || cd_f > 1)
			return Inter_SendClientMessage(playerid,red,"ERROR: Use only 0(NoFreeze) and 1(Freeze)!");
			CdStated = 1;
			if(cd_f == 1)
			for(new i=0;i<MAX_PLAYERS;i++)
			{
				if(IsPlayerConnected(i))
				TogglePlayerControllable(i, 0);
			}
			cd_timer = SetTimer("CountDown",1000,3);
			return 1;
		}
		else return Inter_SendClientMessage(playerid,red,"ERROR: Countdown already in Progress!");
	}
	else return ErrorMessages(playerid, 1);
}


