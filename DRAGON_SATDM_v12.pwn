CMD:getinfo(playerid,params[])
{
    if(AccInfo[playerid][Level] >= 1 || IsPlayerAdmin(playerid))
	{
		new player1;
		if (sscanf(params, "u", player1)) return
		Inter_SendClientMessage(playerid, LIGHTBLUE2, "Usage: /getinfo [PlayerID] ") &&
		Inter_SendClientMessage(playerid, orange, "Function: Will see Informations of specified player");
		new string[128];

	 	if(IsPlayerConnected(player1) && player1 != INVALID_PLAYER_ID)
		{
			new TimesOn;
			new pIP[128];
		    new Float:pHealth;
			new Float:pArmour;
			new year,month,day;
			new pn,Sum, Average;
			new tmp2[512];
			new Float:x,Float:y,Float:z;
			new pRegister[4],RegDate[20];
			new pJailed[4],pFrozen[4];
			new pInCage[4], pLogged[4];
			new LuX_GpsZone[MAX_ZONE_NAME] = "Undetected Zone!";

			GetPlayerArmour(player1,pArmour);
			GetPlayerHealth(player1,pHealth);
	    	GetPlayerIp(player1, pIP, sizeof(pIP));
	    	getdate(year, month, day);
	    	GetPlayerPos(player1,x,y,z);
			GetPlayerInZone(player1, LuX_GpsZone, MAX_ZONE_NAME);

			if(AccInfo[player1][Jailed] == 1)   pJailed   = "Yes"; else pJailed   = "No";
			if(AccInfo[player1][Frozen] == 1) 	pFrozen   = "Yes"; else pFrozen   = "No";
			if(AccInfo[player1][pCaged] == 1) 	pInCage   = "Yes"; else pInCage   = "No";
			if(AccInfo[player1][LoggedIn] == 1) pLogged   = "Yes"; else pLogged   = "No";
			if(DOF2_FileExists(DOF2_File(PlayerName2(player1))))				    pRegister = "Yes"; else pRegister = "No";

			if(strlen(DOF2_GetString(DOF2_File(PlayerName2(player1)), "Laston"))<3) strmid(tmp2,"Never",0,sizeof(tmp2));
			else strmid(tmp2,DOF2_GetString(DOF2_File(PlayerName2(player1)), "Laston"),0,sizeof(tmp2));

			if(strlen(DOF2_GetString(DOF2_File(PlayerName2(player1)), "RegisteredDate"))<3) strmid(RegDate,"n/a",0,sizeof(RegDate));
			else strmid(RegDate,DOF2_GetString(DOF2_File(PlayerName2(player1)), "RegisteredDate"),0,sizeof(RegDate));

			TimesOn = DOF2_GetInt(DOF2_File(PlayerName2(player1)), "TimesOnServer");

			while (pn < PING_MAX_EXCEEDS){
			Sum += AccInfo[player1][pPing][pn]; pn++;
			}
			Average = (Sum / PING_MAX_EXCEEDS);
 			if(IsPlayerAdmin(player1)) AdmRank = "RCON Administrator";
	    	else
 			switch(AccInfo[player1][Level])
			{
				case 0: AdmRank = "Player";
				case 1: AdmRank = "Basic Moderator";
				case 2: AdmRank = "Moderator";
				case 3: AdmRank = "Master Moderator";
				case 4: AdmRank = "Administrator";
				case 5: AdmRank = "Master Administrator";
			}
			if(AccInfo[player1][Level] > 5)
			{
				AdmRank = "Professional Admin";
			}
