public OnPlayerKeyStateChange(playerid, newkeys, oldkeys)
{
//------------------------------LuxAdmin System---------------------------------
	#if ExecuteCmdKey != 0
	if(newkeys == ExecuteCmdKey && AccInfo[playerid][Level] >= 2)
	{
	    return ShowPlayerDialog(playerid, DIALOGID+80, DIALOG_STYLE_INPUT, "LuxAdmin - Execute Command",
