CMD:gang(playerid,params[]) {
	new tmp[256], tmp2[256], Index; tmp = strtok(params,Index), tmp2 = strtok(params,Index);
	if(!strlen(tmp)) return SendClientMessage(playerid, RandomColors[random(200)], "USE: /gang [create/invite/join/leave/stats/list]");

    //---- Create a Gang
 	if(strcmp(tmp, "create", true) == 0)
	{
		if(!strlen(tmp2)) return SendClientMessage(playerid, RandomColors[random(200)], "USE: /gang create [GangName]");
		if(strlen(tmp2) < 3 || strlen(tmp2) > 15) {
			return Inter_SendClientMessage(playerid, RandomColors[random(200)], "Gang name must be between 3 and 15 characters");
		}

		if(AccInfo[playerid][gang] != 0)
		{
			return Inter_SendClientMessage(playerid, RandomColors[random(200)], "You must leave your gang before make a new one. /gang leave");
		}
		//Remove Inactive Gangs
		if(GANG_NUMBER >= MAX_GANG)
		{
			new string[128];
