	format(filestring, sizeof(filestring), "%d,%f,%f,%f,%f,\n",
	ZoneInfo[areaid][ZONE_GANGID],
	ZoneInfo[areaid][ZONE_X1],
	ZoneInfo[areaid][ZONE_Y1],
	ZoneInfo[areaid][ZONE_X2],
	ZoneInfo[areaid][ZONE_Y2]
	);
	new zoneids[5];
	valstr(zoneids,areaid);
	dini_Set(ZONE_FILE,zoneids,filestring);
	ZoneInfo[areaid][ZONE_COLOR] = GangInfo[gangid][GANG_COLOR];

 //---- Rilevamento GangZone
	for(new i; i < MAX_PLAYERS; i++)
	{
		if(IsPlayerConnected(i) && AccInfo[i][gang] == gangid)
	    {
	    	Inter_SendClientMessage(i, RandomColors[random(200)], "Zone Taken");
		}
	}
	for(new i = 0; i <= ZONE_NUMBER; i++)
	{
	    GangZoneDestroy(ZoneInfo[i][ZONE_GZID]);

		if(dini_Exists(ZONE_FILE))
		{
	        new SplitDiv[99][MAX_ZONE];
			new tmp2[256];
			valstr(zoneids,i);
			tmp2 = dini_Get(ZONE_FILE,zoneids);
			split(tmp2, SplitDiv, ',');
		    ZoneInfo[i][ZONE_GANGID] = strval(SplitDiv[0]);
			ZoneInfo[i][ZONE_ID] = i;
			ZoneInfo[i][ZONE_COLOR] = RandomColors[ZoneInfo[i][ZONE_GANGID]];
			ZoneInfo[i][ZONE_X1] = floatstr(SplitDiv[1]);
			ZoneInfo[i][ZONE_Y1] = floatstr(SplitDiv[2]);
			ZoneInfo[i][ZONE_X2] = floatstr(SplitDiv[3]);
			ZoneInfo[i][ZONE_Y2] = floatstr(SplitDiv[4]);
			ZoneInfo[i][ZONE_GZID] = GangZoneCreate(ZoneInfo[i][ZONE_X1], ZoneInfo[i][ZONE_Y1], ZoneInfo[i][ZONE_X2], ZoneInfo[i][ZONE_Y2]);
			GangZoneShowForAll(ZoneInfo[i][ZONE_GZID], ZoneInfo[i][ZONE_COLOR]);
		}
	}
	return 1;
}

public Message2Gang(gangid, text[])
{
	for(new i; i < MAX_PLAYERS; i++)
	{
	    if(AccInfo[i][gang] == gangid && IsPlayerConnected(i)) SendClientMessage(i, GangInfo[gangid][GANG_COLOR], text);
	}
	return 1;
}
// Update the following line to make more areachecks possible.
#define MAX_AREAS_TO_CHECK 200
new Float:AREACHECK_Coords[MAX_AREAS_TO_CHECK][4];
new AREACHECK_Player[MAX_PLAYERS];
new AREACHECK_Count=0;
/*
 *  Return the AreaId, starts with 1 not 0.
 */
stock AddAreaCheck(Float:x_min,Float:x_max,Float:y_min,Float:y_max) {
	AREACHECK_Coords[AREACHECK_Count][0]=x_min;
	AREACHECK_Coords[AREACHECK_Count][1]=x_max;
	AREACHECK_Coords[AREACHECK_Count][2]=y_min;
	AREACHECK_Coords[AREACHECK_Count][3]=y_max;
	AREACHECK_Count++;
	return AREACHECK_Count;
}

timer DCallbacks[1000]() {
	new i,z;
	new Float:x,Float:y,Float:zcoord;
	for (i=0;i<MAX_PLAYERS;i++) {
		if (IsPlayerConnected(i)) {
			GetPlayerPos(i,x,y,zcoord);
			for (z=0;z<AREACHECK_Count;z++) {
				if ( (AREACHECK_Coords[z][0]<x)&&(AREACHECK_Coords[z][1]>x) &&
					(AREACHECK_Coords[z][2]<y)&&(AREACHECK_Coords[z][3]>y) ) {
					if (z!=AREACHECK_Player[i]-1) {
						// Is new in this area
						AREACHECK_Player[i]=z;
						OnPlayerEnterArea(i,z);
					}
				} else {
					if (z==AREACHECK_Player[i]-1) {
						// Player was in this Area
						OnPlayerLeaveArea(i,z);
						AREACHECK_Player[i]=0;
					}
				}
			}
		}
	}
}

