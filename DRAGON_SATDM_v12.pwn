		{
		    KeyState[playerid] = 3;
		}
		else if(newkeys == KEY_WALK)
		{
		    KeyState[playerid] = 4;
		}
		else if(newkeys == (KEY_WALK+KEY_SPRINT))
		{
		    KeyState[playerid] = 5;
		}
		else
		{
		    KeyState[playerid] = 0;
		}
	}
	#endif
	#endif
	//-------------------------------------------------
	if (newkeys & 16 && InAndrom[playerid] > 0)
	{
		new Float:X,Float:Y,Float:Z,Float:A;
		GetVehiclePos(InAndrom[playerid],X,Y,Z);
		GetVehicleZAngle(InAndrom[playerid],A);
		X+=(5*floatsin(-floatsub(A,45.0),degrees)),
