	return Inter_SendClientMessage(playerid,yellow, "Your question has been sent to online administrators.");
}

CMD:helpme(playerid,params[]) {
	#pragma unused params
	if(AccInfo[playerid][Level] ==0)
	{
		if(NeedsHelp[playerid] == 0)
		{
			new asker[MAX_PLAYER_NAME], str[128];
			GetPlayerName(playerid, asker, sizeof(asker));
			format(str, sizeof(str), "||NewReport||  %s(%d) Needs Help! use /helpyou <ID> to help!",asker,playerid);
			NeedsHelp[playerid] = 1;
			MessageToAdmins(COLOR_WHITE,str);
			return Inter_SendClientMessage(playerid,yellow, "Your request for help has been sent to online administrators.");
		}
		else {
			return Inter_SendClientMessage(playerid,yellow, "You have already asked for help, please wait patiently.");
		}
	}
	else {
		return Inter_SendClientMessage(playerid,COLOR_RED, "You are an Admin and cannot ask an admin for help!");
	}
}

CMD:helpyou(playerid,params[]) {
	if(AccInfo[playerid][Level] >1)
	{
		new asker;
		if (sscanf(params, "u", asker)) return Inter_SendClientMessage(playerid, COLOR_LIGHTBLUE, "USAGE: /helpyou <ID>");
		if(NeedsHelp[asker] == 1)
		{
			new giveplayer[MAX_PLAYER_NAME],sendername[MAX_PLAYER_NAME],string[128];
			GetPlayerName(asker, giveplayer, sizeof(giveplayer));
			GetPlayerName(playerid, sendername, sizeof(sendername));
			format(string, sizeof(string), "%s has answered your call for help, talk away", sendername);
			Inter_SendClientMessage(asker, COLOR_GREEN, string);
			format(string, sizeof(string), "You have answered %s's call for help, talk away", giveplayer);
			Inter_SendClientMessage(playerid, COLOR_GREEN, string);
			NeedsHelp[asker] = 0;
			Helping[playerid] = asker;
			Helping[asker] = playerid;
			return 1;
		}
		else {
			return Inter_SendClientMessage(playerid,yellow, "This player doesn't need help!");
		}
	}
	else {
		return Inter_SendClientMessage(playerid,COLOR_RED, "You must be at least Admin level 2 to help players!");
	}
}

