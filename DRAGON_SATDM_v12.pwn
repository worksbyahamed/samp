		    format(str,sizeof(str),"12AKA: [%s id:%d] [%s] %s", playername, playerid, tmp3, dini_Get("/LuxAdmin/Config/aka.txt",tmp3) );
	        return IRC_GroupSay(gGroupID, channel, str);
