//----------------------------------Create a Ramp-------------------------------
CMD:ramp(playerid,params[]) {
	#pragma unused params
	if(!IsPlayerInAnyVehicle(playerid))
	{
		Inter_SendClientMessage(playerid,COLOR_RED,"You need to be in a vehicle!");
		return 1;
	}
	if (rampa[playerid] == 1)
	{
		rampa[playerid] = 0;
		Inter_SendClientMessage(playerid,COLOR_RED,"RAMPS OFF");
	}
	else
	{
		rampa[playerid] = 1;
		Inter_SendClientMessage(playerid,COLOR_GREEN,"RAMPS ON");
	}
	return 1;
}
