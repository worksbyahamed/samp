CMD:search(playerid,params[]) {
	new giveplayerid,string[128];
	if (sscanf(params, "u", giveplayerid)) return Inter_SendClientMessage(playerid, COLOR_LIGHTBLUE, "USAGE: /search <ID>");
//	if (!IsNumeric(giveplayerid)) giveplayerid = ReturnPlayerID(giveplayerid);
	if(IsSpawned[playerid] == 0) {
		Inter_SendClientMessage(playerid, COLOR_ERROR, "You are dead. You cannot use this command");
		return 1;
	}
	if(AccInfo[playerid][Jailed] == 1) {
		Inter_SendClientMessage(playerid, COLOR_ERROR, "You cannot use this command in jail");
		return 1;
	}
	if(AccInfo[playerid][team] != TEAM_LVFBI && AccInfo[playerid][team] != TEAM_LVCOP && AccInfo[playerid][team] != TEAM_LSCOP && AccInfo[playerid][team] != TEAM_SWAT && AccInfo[playerid][team] != TEAM_ARMY && AccInfo[playerid][team] != TEAM_LVARMY) {
		Inter_SendClientMessage(playerid,COLOR_ERROR,"You are not a Law Enforcement agent");
		return 1;
	}
	if(!IsPlayerConnected(giveplayerid)) {
		format(string, sizeof(string), "ID (%d) is not an active player", giveplayerid);
		Inter_SendClientMessage(playerid, COLOR_ERROR, string);
		return 1;
	}
	new officername[24];
	new civilname[24];
	GetPlayerName(playerid,officername, 24);
	GetPlayerName(giveplayerid,civilname, 24);
	if(GetDistanceBetweenPlayers(playerid,giveplayerid) > 5) {
		format(string, sizeof(string), "%s(%d) Is not close enough. You cannot search that player for weed",civilname,giveplayerid);
		Inter_SendClientMessage(playerid, COLOR_ERROR, string);
		return 1;
	}
	if(cuffed[giveplayerid] == 0) {
		format(string, sizeof(string), "%s(%d) Is not handcuffed. Place the suspect in handcuffs before searching for weed",civilname,giveplayerid);
		Inter_SendClientMessage(playerid, COLOR_ERROR, string);
		return 1;
	}
	if(IsSpawned[giveplayerid] == 0) {
		format(string, sizeof(string), "%s(%d) Is dead. You cannot search a dead body",civilname,giveplayerid);
		Inter_SendClientMessage(playerid, COLOR_ERROR, string);
		return 1;
	}
	if(IsPlayerInAnyVehicle(giveplayerid)) {
		format(string, sizeof(string), "%s(%d) Is in a vehicle. Get the suspect out before searching for weed",civilname,giveplayerid);
		Inter_SendClientMessage(playerid, COLOR_ERROR, string);
		return 1;
	}
	Inter_SendClientMessage(playerid, 0xA9A9A9AA, "|_Suspect Searched_|");
	Inter_SendClientMessage(giveplayerid, 0xA9A9A9AA, "|_Law Enforcement Stop Search_|");
	if(AccInfo[giveplayerid][weed] <= 0) {
		format(string, sizeof(string), "%s(%d) Has been searched. No weed was found",civilname,giveplayerid);
		Inter_SendClientMessage(playerid, 0x00C7FFAA, string);
		format(string, sizeof(string), "Officer %s(%d) Has searched you for weed. You are not carrying any weed. No weed was found",officername,playerid);
		Inter_SendClientMessage(giveplayerid, 0x00C7FFAA, string);
	}
	if(AccInfo[giveplayerid][weed] >=1 && HasWeedBag[giveplayerid] == 1) {
		format(string, sizeof(string), "%s(%d) Has been searched. No weed was found. The suspect is free to go",civilname,giveplayerid);
		Inter_SendClientMessage(playerid, 0x00C7FFAA, string);
		format(string, sizeof(string), "Officer %s(%d) Has searched you for weed. You have a WeedBag. Your weed was not found",officername,playerid);
		Inter_SendClientMessage(giveplayerid, 0x00C7FFAA, string);
	}
	if(AccInfo[giveplayerid][weed] >=1 && HasWeedBag[giveplayerid] == 0) {
		new weedpossesed = (AccInfo[giveplayerid][weed]);
		format(string, sizeof(string), "%s(%d) Has been searched. You found %d gram(s) of weed. You have siezed the weed and receive a $2000 bonus",civilname,giveplayerid,weedpossesed);
		Inter_SendClientMessage(playerid, 0x00C7FFAA, string);
		format(string, sizeof(string), "Officer %s(%d) Has searched you for weed. %d gram(s) of weed were found and siezed",officername,playerid,weedpossesed);
		Inter_SendClientMessage(giveplayerid, COLOR_ERROR, string);
		new plwl = GetPlayerWantedLevel(giveplayerid);
		SetPlayerWantedLevel(giveplayerid, plwl +4 );
		new pcol = GetPlayerColor(giveplayerid);
		plwl = GetPlayerWantedLevel(giveplayerid);
		format(string, sizeof(string), "Officer %s(%d) has searched %s(%d)  - %d gram(s) of weed were found during the search",officername,playerid,civilname,giveplayerid,weedpossesed);
		Inter_SendClientMessageToAll(0x00C7FFAA, string);
		printf("%s", string);
		Inter_SendClientMessage(giveplayerid, 0xA9A9A9AA, "|_Crime Commited_|");
		format(string, sizeof(string), "(WEED POSSESION) Wanted Level %d",plwl);
		Inter_SendClientMessage(giveplayerid,pcol,string);
		SetPlayerScore(playerid, GetPlayerScore(playerid) +1);
		AccInfo[giveplayerid][weed] =0;
		GivePlayerCash(playerid,2000);
	}
	return 1;
}
