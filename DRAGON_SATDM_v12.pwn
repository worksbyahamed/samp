	SetPlayerWantedLevel(playerid, plwl +2 );
	new pcol = GetPlayerColor(playerid);
	plwl = GetPlayerWantedLevel(playerid);
	Inter_SendClientMessage(playerid, 0xA9A9A9AA, "|_Crime Commited_|");
