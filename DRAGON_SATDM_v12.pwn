//==================================Count Timer=================================
public ttimer(playerid)
{
	if(InDM[playerid]==1) tseconds[playerid]=0,TogglePlayerControllable(playerid,0), SetTimerEx("UnfreezeTele",1000, 0, "id", playerid,-1), KillTimer(TeleTimer[playerid]), Inter_SendClientMessage(playerid, COLOR_RED, "You must leave the DM with /exitdm first!");
	if(instunt[playerid]==1) tseconds[playerid]=0,TogglePlayerControllable(playerid,0), SetTimerEx("UnfreezeTele",1000, 0, "id", playerid,-1), KillTimer(TeleTimer[playerid]), Inter_SendClientMessage(playerid, COLOR_RED, "You must leave the Stunt with /kill first!");
    if (tseconds[playerid])
    {
		Streamer_UpdateEx(playerid,Tx[playerid],Ty[playerid],Tz[playerid]);
        format(tstring,6,"~w~%d", tseconds[playerid]-1);
        GameTextForPlayer(playerid,tstring,1100,4);
        tseconds[playerid] --;
		if(AccInfo[playerid][Level] >= 1) tseconds[playerid] = 0;
        if (!tseconds[playerid])
        {
            new vsid = GetPlayerVehicleID(playerid);
            new hasacar = -1;
			if (Ti[playerid]>=0) SetPlayerInterior(playerid,Ti[playerid]);
			if (Tw[playerid]>=0) SetPlayerVirtualWorld(vsid,Tw[playerid]);
			if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER) hasacar=vsid,SetPlayerPos(playerid,Tx[playerid],Ty[playerid],Tz[playerid]), LinkVehicleToInterior(vsid,Ti[playerid]), SetVehicleVirtualWorld(vsid,Tw[playerid]);
			else SetPlayerPos(playerid,Tx[playerid],Ty[playerid],Tz[playerid]);
			GameTextForPlayer(playerid,telestring,2500,3);
			KillTimer(TeleTimer[playerid]);
			Ti[playerid]=-1;
			Tw[playerid]=-1;
			TogglePlayerControllable(playerid,false);
			SetTimerEx("UnfreezeTele",2000, 0, "id", playerid,hasacar);
        }
    }
}

stock TimedTele(playerid, telename[], Float:Px, Float:Py, Float:Pz, interior, world, seconds)
{
	if(AccInfo[playerid][Jailed] == 1) return Inter_SendClientMessage(playerid,COLOR_BRIGHTRED,"You cannot escape your punishment!");
	TeleTimer[playerid] = SetTimerEx("ttimer", 1000, true, "i",playerid);
	tseconds[playerid] = seconds;
	format(tstring,45,"You Must wait %d seconds before teleporting", tseconds[playerid]-1);
	if(AccInfo[playerid][Level] == 0) Inter_SendClientMessage(playerid,COLOR_LIGHTBLUE, tstring);
	Tx[playerid] =Px;
	Ty[playerid] =Py;
	Tz[playerid] =Pz;
	Ti[playerid] = interior;
	Tw[playerid] = world;
	TogglePlayerControllable(playerid,false);
	format(telestring,43,telename);
	return 1;
}
//==================================Count Timer=================================
public ctimer()
{
	if (cseconds)
	{
		format(cstring,6,"~w~%d", cseconds-1);
		GameTextForAll(cstring,1100,4);
		SoundForAll(1056);
		cseconds --;
		if (!cseconds)
		{
			GameTextForAll("~r~GO GO GO!!!",2000,4);
			SoundForAll(1057);
			KillTimer(CountingTimer);
			for (new i = 0; i < GetMaxPlayers(); i ++)
			{
				if (IsPlayerConnected(i)) SetTimer("resetcount",2000,0);
			}
		}
	}
}

stock SoundForAll(sound)
{
	for (new i = 0; i <= GetMaxPlayers(); i ++)
	{
		if (IsPlayerConnected(i) == 1) {
			PlayerPlaySound(i,sound,0.0,0.0,0.0);
		}
	}
}
