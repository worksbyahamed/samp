		return 1;
	}
	else SendClientMessage(playerid, RandomColors[random(200)], "USE: /gang [create/invite/join/leave/stats/list]");
	return 1;
}

CMD:createzone(playerid,params[]) {
	#pragma unused params
	if (!IsPlayerAdmin(playerid)) return Inter_SendClientMessage(playerid,0x247C1BFF,"* You Must be rcon admin to do that!");
	if(creating == 0) ZONE_NUMBER++;
	if(zoneid == 0)
	{
		for(new i = 0; i < ZONE_NUMBER; i++)
  		{
			if(!dini_Exists(ZONE_FILE) && i != 0) zoneid = i;
		}
	}
	if(creating == 0)
	{
		dini_IntSet(CFG,"ZONE_NUMBER",ZONE_NUMBER);
		ZoneInfo[zoneid][ZONE_ID] = zoneid;
		ZoneInfo[zoneid][ZONE_GANGID] = 0;
		ZoneInfo[zoneid][ZONE_COLOR] = GREY;
		GetPlayerPos(playerid, gx1, gy1, gz1);
		Inter_SendClientMessage(playerid, RandomColors[random(200)], "Rerun the cmd in the opposite corner of the zone");
		creating = 1;
	}
	else
	{
	    GetPlayerPos(playerid, gx2, gy2, gz2);
	    if(gx1 < gx2)
		{
			ZoneInfo[zoneid][ZONE_X1] = gx1;
			ZoneInfo[zoneid][ZONE_X2] = gx2;
		}
		else
		{
			ZoneInfo[zoneid][ZONE_X1] = gx2;
			ZoneInfo[zoneid][ZONE_X2] = gx1;
		}
		if(gy1 < gy2)
		{
			ZoneInfo[zoneid][ZONE_Y1] = gy1;
			ZoneInfo[zoneid][ZONE_Y2] = gy2;
		}
		else
		{
			ZoneInfo[zoneid][ZONE_Y1] = gy2;
			ZoneInfo[zoneid][ZONE_Y2] = gy1;
		}
		new filestring[64];
		format(filestring, sizeof(filestring), "%d,%f,%f,%f,%f,\n",
		0,
		ZoneInfo[zoneid][ZONE_X1],
		ZoneInfo[zoneid][ZONE_Y1],
		ZoneInfo[zoneid][ZONE_X2],
		ZoneInfo[zoneid][ZONE_Y2]
		);
		new zoneids[5];
		valstr(zoneids,zoneid);
		dini_Set(ZONE_FILE,zoneids,filestring);

		ZoneInfo[zoneid][ZONE_GZID] = GangZoneCreate(gx1, gy1, gx2, gy2);
		GangZoneShowForAll(ZoneInfo[zoneid][ZONE_GZID], GREY);
		AddAreaCheck(ZoneInfo[zoneid][ZONE_X1], ZoneInfo[zoneid][ZONE_X2], ZoneInfo[zoneid][ZONE_Y1], ZoneInfo[zoneid][ZONE_Y2]);
		creating = 0;
		new string[64];
		format(string, sizeof(string), "GangZone Created. ID: %d", zoneid);
		Inter_SendClientMessage(playerid, RandomColors[random(200)], string);
		zoneid = 0;
	}
	return 1;
}
