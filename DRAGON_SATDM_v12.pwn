			format(string, sizeof(string), "You have finished answering %s's call for help, and have hung up.", giveplayer);
			Inter_SendClientMessage(playerid, COLOR_GREEN, string);
			Helping[Helping[playerid]] = -1;
			Helping[playerid] = -1;
			return 1;
		}
		else {
			return Inter_SendClientMessage(playerid,yellow, "You are not helping anyone!");
