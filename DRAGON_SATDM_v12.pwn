	for(new g = 0; g <= GANG_NUMBER; g++)
	{
		new file[100];
		format(file, sizeof(file), GANG_FILE, g);
		if(dini_Exists(file)) {
			GangInfo[g][GANG_ID] = g;
			strcat(GangInfo[g][GANG_NAME], dini_Get(file, "GANG_NAME"),strlen(dini_Get(file, "GANG_NAME"))+1);
			GangInfo[g][GANG_MEMBERS] = dini_Int(file, "GANG_MEMBERS");
			GangInfo[g][GANG_BANK] = dini_Int(file, "GANG_BANK");
			GangInfo[g][GANG_COLOR] = RandomColors[g];
			for(new l; l < ZONE_NUMBER; l++)
			{
			    new SplitDiv2[99][MAX_ZONE];
				new tmp2[256];
				new zoneids[5];
				valstr(zoneids,l);
				tmp2 = dini_Get(ZONE_FILE,zoneids);
				split(tmp2, SplitDiv2, ',');
			    if(dini_Exists(file) && GangInfo[g][GANG_ID] == strval(SplitDiv2[0])) GangInfo[g][GANG_NZONE]++;
			}
		}
	}
	for(new z = 0; z <= ZONE_NUMBER; z++)
	{
		if(dini_Exists(ZONE_FILE))
		{
		    new SplitDiv[99][MAX_ZONE];
			new tmp2[256];
			new zoneids[5];
			valstr(zoneids,z);
			tmp2 = dini_Get(ZONE_FILE,zoneids);
			split(tmp2, SplitDiv, ',');
		    ZoneInfo[z][ZONE_GANGID] = strval(SplitDiv[0]);
			ZoneInfo[z][ZONE_ID] = z;
			ZoneInfo[z][ZONE_COLOR] = RandomColors[ZoneInfo[z][ZONE_GANGID]];
			ZoneInfo[z][ZONE_X1] = floatstr(SplitDiv[1]);
			ZoneInfo[z][ZONE_Y1] = floatstr(SplitDiv[2]);
			ZoneInfo[z][ZONE_X2] = floatstr(SplitDiv[3]);
			ZoneInfo[z][ZONE_Y2] = floatstr(SplitDiv[4]);

			ZoneInfo[z][ZONE_GZID] = GangZoneCreate(ZoneInfo[z][ZONE_X1], ZoneInfo[z][ZONE_Y1], ZoneInfo[z][ZONE_X2], ZoneInfo[z][ZONE_Y2]);
			GangZoneShowForAll(ZoneInfo[z][ZONE_GZID], ZoneInfo[z][ZONE_COLOR]);
