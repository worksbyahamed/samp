GetBestScore(&bestid,&best,toesclude=99999999)
{
	new best2=0;
	new bestid2=INVALID_PLAYER_ID;
	for(new i=0;i<GetMaxPlayers();i++)
	{
		if(GetPlayerScore(i)>best2&&GetPlayerScore(i)<toesclude)
		{
			best2=GetPlayerScore(i);
			bestid2=i;
		}
	}
	bestid=bestid2;
	best=best2;
}


