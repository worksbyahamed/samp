OnPlayerEnterArea(playerid,areaid) {
	if(strlen(GangInfo[ZoneInfo[areaid][ZONE_GANGID]][GANG_NAME]) <3) format( zwm[playerid], sizeof(zwm), "Unowned Turf.");
	else format( zwm[playerid], sizeof(zwm), "%s Turf.", GangInfo[ZoneInfo[areaid][ZONE_GANGID]][GANG_NAME]);
	new string[128];
	new gangid = AccInfo[playerid][gang];
	if(gangid != ZoneInfo[areaid][ZONE_GANGID] && gangid != 0 && GetPlayerVirtualWorld(playerid) == 0 && PlayerInZone[playerid] != areaid) Invaders[gangid][areaid]++, PlayerInZone[playerid] = areaid; //Aggiunta di un invasore
	if(Invaders[gangid][areaid] >= 3 && Taking[gangid][areaid] == 0)
	{
	    for(new i; i < MAX_PLAYERS; i++)
	    {
	        if(IsPlayerConnected(i) && AccInfo[i][gang] == gangid)
	        {
	            Inter_SendClientMessage(i, RandomColors[random(200)], "You are taking the gang zone");
			}
		}
		Taking[gangid][areaid] = 1;
	    GangZoneFlashForAll(ZoneInfo[areaid][ZONE_GZID],GangInfo[gangid][GANG_COLOR]);
	    if(ZoneInfo[areaid][ZONE_GANGID] != 0) GangTimer[gangid][areaid] = SetTimerEx("ConquerGZ", 60000*2, false, "ii", areaid, gangid);
		else GangTimer[gangid][areaid] = SetTimerEx("ConquerGZ", 30000, false, "ii", areaid, gangid);
	    for(new i; i < MAX_PLAYERS; i++)
	    {
	        if(IsPlayerConnected(i) && AccInfo[i][gang] == ZoneInfo[areaid][ZONE_GANGID] && AccInfo[playerid][gang] != ZoneInfo[areaid][ZONE_GANGID] && AccInfo[i][gang] != 0)
	        {
				Inter_SendClientMessage(i, RandomColors[random(200)], "One of you gang's zones is under attack");
				Inter_SendClientMessage(i, RandomColors[random(200)], "One of you gang's zones is under attack");
				Inter_SendClientMessage(i, RandomColors[random(200)], "One of you gang's zones is under attack");
	            format(string, sizeof(string), "X: %f, Y: %f, Area ID: %d", ZoneInfo[areaid][ZONE_X1], ZoneInfo[areaid][ZONE_Y1], areaid);
	            if(IsPlayerAdmin(i)) Inter_SendClientMessage(i, RandomColors[random(200)], string);
			}
		}
	}
}

OnPlayerLeaveArea(playerid,areaid) {
	format(zwm[playerid], sizeof(zwm), "No Turf.");
	if(Invaders[AccInfo[playerid][gang]][areaid] >= 3 && Taking[AccInfo[playerid][gang]][areaid] == 1)
	{
		Taking[AccInfo[playerid][gang]][areaid] = 0;
		KillTimer(GangTimer[AccInfo[playerid][gang]][areaid]);
		GangZoneStopFlashForAll(ZoneInfo[areaid][ZONE_GZID]);
	}
	if(AccInfo[playerid][gang] != ZoneInfo[areaid][ZONE_GANGID] && AccInfo[playerid][gang] != 0 && GetPlayerVirtualWorld(playerid) == 0) Invaders[AccInfo[playerid][gang]][areaid]--;
}

