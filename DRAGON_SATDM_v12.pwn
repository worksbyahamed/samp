public HighLight(playerid)
{
	if(!IsPlayerConnected(playerid))
	return 1;
	if(AccInfo[playerid][blipS] == 0)
	{
		SetPlayerColor(playerid, 0xFF0000AA);
		AccInfo[playerid][blipS] = 1;
	}
	else
	{
		SetPlayerColor(playerid, 0xFFFF00AA);
		AccInfo[playerid][blipS] = 0;
	}
	return 0;
}
