CMD:ping(playerid,params[])
{
	if(AccInfo[playerid][Level] >= 1)
	{
	    if(isnull(params)) return
		Inter_SendClientMessage(playerid, LIGHTBLUE2, "Usage: /ping [PlayerID]") &&
		Inter_SendClientMessage(playerid, orange, "Function: Will see the Ping Informations of the specified player");
		new player1 = strval(params), string[128];
        if(IsPlayerConnected(player1) && player1 != INVALID_PLAYER_ID)
		{
		    new Sum, Average, x;
			while (x < PING_MAX_EXCEEDS)
			{
				Sum += AccInfo[player1][pPing][x];
				x++;
			}
			Average = (Sum / PING_MAX_EXCEEDS);
			format(string, sizeof(string), "|- \"%s\" (Id: %d) Average Ping: %d - Last Readings: %d, %d, %d, %d", PlayerName2(player1), player1, Average, AccInfo[player1][pPing][0], AccInfo[player1][pPing][1], AccInfo[player1][pPing][2], AccInfo[player1][pPing][3] );
			return Inter_SendClientMessage(playerid,blue,string);
		}
		else return ErrorMessages(playerid, 2);
	}
	else return ErrorMessages(playerid, 1);
}
