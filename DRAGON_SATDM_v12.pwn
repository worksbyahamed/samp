//---------------------------------Call on Phone--------------------------------
CMD:call(playerid,params[]) {
	new giveplayerid,string[128],giveplayer[MAX_PLAYER_NAME],sendername[MAX_PLAYER_NAME];
	if (sscanf(params, "u", giveplayerid)) return Inter_SendClientMessage(playerid, COLOR_LIGHTBLUE, "USAGE: /call <ID>");
//	if (!IsNumeric(giveplayerid)) giveplayerid = ReturnPlayerID(giveplayerid);
	if (AccInfo[playerid][LoggedIn] != 1)
	{
		Inter_SendClientMessage(playerid, COLOR_BRIGHTRED, "You must be logged in to make a phone call!");
		return 1;
	}
	if (GetPlayerCash(playerid) < CALL_UNIT_COST)
	{
		Inter_SendClientMessage(playerid, COLOR_BRIGHTRED, "You do not have enough money to make a phone call!");
		return 1;
	}
	if (giveplayerid == playerid)
	{
		Inter_SendClientMessage(playerid, COLOR_BRIGHTRED, "You cannot call yourself!");
		return 1;
	}
	if (!(IsPlayerConnected(giveplayerid)))
	{
		Inter_SendClientMessage(playerid, COLOR_BRIGHTRED, "Inactive player id!");
		return 1;
	}
	if (Calling[playerid] > -1)
	{
		GetPlayerName(Calling[playerid], giveplayer, sizeof(giveplayer));
		format(string, sizeof(string), "You are already on the phone to %s!", giveplayer);
		Inter_SendClientMessage(playerid, COLOR_BRIGHTRED, string);
		return 1;
	}
	if (Calling[giveplayerid] > -1)
	{
		GetPlayerName(giveplayerid, giveplayer, sizeof(giveplayer));
		format(string, sizeof(string), "%s is already on the phone, try again later", giveplayer);
		Inter_SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
		return 1;
	}
	else
	{
		GetPlayerName(giveplayerid, giveplayer, sizeof(giveplayer));
		GetPlayerName(playerid, sendername, sizeof(sendername));
		format(string, sizeof(string), "%s is ringing you, type /answer to answer the call or /hangup to ignore it", sendername);
		Inter_SendClientMessage(giveplayerid, COLOR_LIGHTBLUE, string);
		format(string, sizeof(string), "You are ringing %s", giveplayer);
		Inter_SendClientMessage(playerid, COLOR_GREEN, string);
		Calling[playerid] = giveplayerid;
		Calling[giveplayerid] = playerid;
		Callerid[playerid] = 1;
		SetPlayerSpecialAction(playerid,SPECIAL_ACTION_USECELLPHONE);
		SetPlayerAttachedObject(playerid, 4, 330, 6);
	}
	return 1;
}
//---------------------------------Answer Phone---------------------------------
CMD:answer(playerid,params[]) {
	#pragma unused params
	new string[128],giveplayer[MAX_PLAYER_NAME],sendername[MAX_PLAYER_NAME];
	if (AccInfo[playerid][LoggedIn] != 1)
	{
		Inter_SendClientMessage(playerid, COLOR_BRIGHTRED, "You must be logged in to answer a phone call!");
		return 1;
	}
	if (Calling[playerid] == -1)
	{
		Inter_SendClientMessage(playerid, COLOR_BRIGHTRED, "Nobody is calling you!");
		return 1;
	}
	if (Answered[playerid] == 1)
	{
		Inter_SendClientMessage(playerid, COLOR_LIGHTBLUE, "You are already on the phone!");
		return 1;
	}
	else
	{
		GetPlayerName(Calling[playerid], giveplayer, sizeof(giveplayer));
		GetPlayerName(playerid, sendername, sizeof(sendername));
		format(string, sizeof(string), "%s has answered your call, talk away", sendername);
		Inter_SendClientMessage(Calling[playerid], COLOR_GREEN, string);
		format(string, sizeof(string), "You have answered %s's call, talk away", giveplayer);
		Inter_SendClientMessage(playerid, COLOR_GREEN, string);
   		Answered[playerid] = 1;
   		Answered[Calling[playerid]] = 1;
		Callerid[Calling[playerid]] = 1;
		SetPlayerSpecialAction(playerid,SPECIAL_ACTION_USECELLPHONE);
		SetPlayerAttachedObject(playerid, 4, 330, 6);
	}
	return 1;
}
//---------------------------------HangUp Phone---------------------------------
CMD:hangup(playerid,params[]) {
	#pragma unused params
	new string[128],giveplayer[MAX_PLAYER_NAME],sendername[MAX_PLAYER_NAME];
	if (AccInfo[playerid][LoggedIn] != 1)
	{
		Inter_SendClientMessage(playerid, COLOR_BRIGHTRED, "You must be logged in to hang up a phone call!");
		return 1;
	}
	if (Calling[playerid] == -1)
	{
		Inter_SendClientMessage(playerid, COLOR_BRIGHTRED, "You are not on the phone!");
		return 1;
	}
	else
	{
		GetPlayerName(Calling[playerid], giveplayer, sizeof(giveplayer));
		GetPlayerName(playerid, sendername, sizeof(sendername));
		format(string, sizeof(string), "%s has hung up the phone on you", sendername);
		Inter_SendClientMessage(Calling[playerid], COLOR_LIGHTBLUE, string);
		format(string, sizeof(string), "You have hung up on %s", giveplayer);
		Inter_SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
		new bCalling = Calling[playerid];
		Calling[playerid] = -1;
		Answered[playerid] = 0;
		Callerid[playerid] = 0;
		Calling[bCalling] = -1;
		Answered[bCalling] = 0;
		Callerid[bCalling] = 0;
		SetPlayerSpecialAction(Calling[playerid],SPECIAL_ACTION_STOPUSECELLPHONE);
		RemovePlayerAttachedObject(Calling[playerid], 4);
		SetPlayerSpecialAction(playerid,SPECIAL_ACTION_STOPUSECELLPHONE);
		RemovePlayerAttachedObject(playerid, 4);
	}
	return 1;
}
