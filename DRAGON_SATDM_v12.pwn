	#pragma unused params
	if(AccInfo[playerid][Level] >1)
	{
		if(Helping[playerid] > -1)
		{
			new giveplayer[MAX_PLAYER_NAME],sendername[MAX_PLAYER_NAME],string[128];
			GetPlayerName(Helping[playerid], giveplayer, sizeof(giveplayer));
			GetPlayerName(playerid, sendername, sizeof(sendername));
			format(string, sizeof(string), "%s has finished helping you and has hung up", sendername);
