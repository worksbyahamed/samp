//==============================Gamemode exit stuff=============================
public OnGameModeExit()
{
//------------------------------------Gangs-------------------------------------
    for(new i = 0; i <= ZONE_NUMBER; i++)
	{
  		GangZoneDestroy(ZoneInfo[i][ZONE_GZID]);
	}
//-------------------------------------IRC--------------------------------------
	IRC_Quit(gBotID[0], "BoT 1 ExIsTeD");// Disconnect the first bot
	IRC_Quit(gBotID[1], "BoT 2 ExIsTeD");// Disconnect the second bot
	IRC_DestroyGroup(gGroupID);// Destroy the group
//------------------------------------Gates-------------------------------------
	DestroyAllDynamicObjects();
	KillTimer(GGateTimer);
//----------------------------------Streamer------------------------------------
//	DestroyAllDynamicPickups();
	DestroyAllDynamicMapIcons();
	DestroyAllDynamic3DTextLabels();
//------------------------------LuxAdmin System---------------------------------
	for(new i = 0; i < MAX_PLAYERS; i++)
    TextDrawHideForPlayer(i, Announcements),
