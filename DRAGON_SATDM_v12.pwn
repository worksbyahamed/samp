public SystemMsg(playerid,msg[])
{
	if ((IsPlayerConnected(playerid))&&(strlen(msg)>0))
	{
		Inter_SendClientMessage(playerid,COLOR_SYSTEM,msg);
	}
	return 1;
}
//===============================Kick timer=====================================
public KickTimer(playerid)
{
	if(AccInfo[playerid][LoggedIn] == 0) {
		Inter_SendClientMessage(playerid,COLOR_BRIGHTRED, "You have been kicked from the server for Login Timeout!");
		new kickmess[128];
		format(kickmess,sizeof(kickmess),"%s was kicked from the server. Reason: Login Timeout!",PlayerName2(playerid));
		Inter_SendClientMessage(playerid,COLOR_BRIGHTRED,kickmess);
		Kick(playerid);
