 	if(AccInfo[player1][Level] == ServerInfo[MaxAdminLevel] && AccInfo[playerid][Level] != ServerInfo[MaxAdminLevel])
	return Inter_SendClientMessage(playerid,red,"ERROR: You can not localize an Administrator!");

