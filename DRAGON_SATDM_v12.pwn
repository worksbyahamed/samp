		if(AccInfo[giveplayerid][team] == TEAM_LVFBI || AccInfo[giveplayerid][team] == TEAM_LVCOP || AccInfo[giveplayerid][team] == TEAM_LSCOP || AccInfo[giveplayerid][team] == TEAM_SWAT || AccInfo[giveplayerid][team] == TEAM_ARMY || AccInfo[giveplayerid][team] == TEAM_LVARMY) {
