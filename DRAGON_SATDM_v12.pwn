//==========Anti IP advertising By: Joe Bullet(samp-scripting@in.com)===========
stock bool: IsIPInText(txt[])
{
	new
		pos = 0,
		_1,
		_2,
		_3,
		_4,
		str[9],
		delims[] =
		{
			';', 	'|',
			',',    ' ',
			'.',	'-',
			'~',	'*',
			'/',    EOS
		};
	while(txt[pos])
	{
		for(new _n = 0; delims[_n] != EOS; _n++)
		{
			format(str, sizeof(str), "p<%c>dddd", delims[_n]);
			if(!sscanf(txt[pos], str, _1, _2, _3, _4))
			{
				if(0 <= _1 <= 255 &&
				   0 <= _2 <= 255 &&
				   0 <= _3 <= 255 &&
				   0 <= _4 <= 255)
				{
					return true;
				}
			}
		}
		pos++;
	}
	return false;
}
