	Inter_SendClientMessage(playerid,pcol,string);
	return 1;
}
CMD:takeweed(playerid,params[]) {
	new string[128],gramstotake;
	if (sscanf(params, "i",gramstotake)) return Inter_SendClientMessage(playerid, COLOR_LIGHTBLUE, "USAGE: /takeweed <Grams>");
	if(IsSpawned[playerid] == 0) {
		Inter_SendClientMessage(playerid, COLOR_ERROR, "You are dead. You cannot use this command");
		return 1;
	}
	if(AccInfo[playerid][Jailed] == 1) {
		Inter_SendClientMessage(playerid, COLOR_ERROR, "You cannot use this command in jail");
		return 1;
	}
	if(cuffed[playerid] == 1) {
		Inter_SendClientMessage(playerid,COLOR_ERROR,"You are handcuffed. You cannot use this command");
		return 1;
	}
	if(AccInfo[playerid][team] == TEAM_LVFBI || AccInfo[playerid][team] == TEAM_LVCOP || AccInfo[playerid][team] == TEAM_LSCOP || AccInfo[playerid][team] == TEAM_SWAT || AccInfo[playerid][team] == TEAM_ARMY || AccInfo[playerid][team] == TEAM_LVARMY) {
		Inter_SendClientMessage(playerid,COLOR_ERROR,"Law Enforcement Agents cannot get high on weed");
		return 1;
	}
	if(playeronweed[playerid] >= 1) {
		Inter_SendClientMessage(playerid,COLOR_ERROR,"You are already high on weed. Please wait before taking more");
		return 1;
	}
	if(AccInfo[playerid][weed] == 0) {
		Inter_SendClientMessage(playerid,COLOR_ERROR,"You are not carrying any weed. Call for weed with /weed if you want some weed");
		return 1;
	}
	if(AccInfo[playerid][weed] < gramstotake ) {
		Inter_SendClientMessage(playerid,COLOR_ERROR,"You are not carrying that much weed");
		return 1;
	}
	if(gramstotake <= 4 || gramstotake >=51) {
		Inter_SendClientMessage(playerid,COLOR_ERROR,"You can only take from 5 - 50 grams of weed");
		return 1;
	}
	if(gramstotake ==5) {playertookweed[playerid] =1; playeronweed[playerid] =15;}
	else if(gramstotake >=6 && gramstotake <=10)   {playertookweed[playerid] =1; playeronweed[playerid] =30;}
	else if(gramstotake >=11 && gramstotake <=20) {playertookweed[playerid] =1; playeronweed[playerid] =45;}
	else if(gramstotake >=21 && gramstotake <=30) {playertookweed[playerid] =1; playeronweed[playerid] =60;}
	else if(gramstotake >=31 && gramstotake <=40) {playertookweed[playerid] =1; playeronweed[playerid] =120;}
	else if(gramstotake >=41 && gramstotake <=50) {playertookweed[playerid] =1; playeronweed[playerid] =180;}
	AccInfo[playerid][weed] -= gramstotake;
	Inter_SendClientMessage(playerid, 0xA9A9A9AA, "|_Weed Taken_|");
	format(string, sizeof(string), "You are high on weed. You have took %d grams of weed. Your health will increase while you are high on weed",gramstotake);
	Inter_SendClientMessage(playerid, 0x00C7FFAA, string);
	ApplyAnimation(playerid,"SMOKING", "M_smklean_loop", 4.0, 0, 0, 0, 0, 0); // Smoke
   	return 1;
}
