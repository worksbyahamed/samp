	if(AccInfo[playerid][Jailed] == 1) {
		Inter_SendClientMessage(playerid, COLOR_ERROR, "You cannot use this command in jail");
		return 1;
	}
	if(HasLawEnforcementRadio[playerid] == 0) {
		Inter_SendClientMessage(playerid,COLOR_ERROR,"You do not have a Law Enforcement radio");
		return 1;
	}
	if(LawEnforcementRadio[playerid] == 0) {
		Inter_SendClientMessage(playerid, COLOR_ERROR, "Your Law Enforcement radio is off. Turn it on with /radon before using it");
		return 1;
	}
	else{
		new pname[24];
		GetPlayerName(playerid, pname, 24);
		format(string, sizeof(string), "COP MESSAGE: %s(%d) %s",pname,playerid,output);
		printf("%s", string);
		for(new i=0;i<GetMaxPlayers();i++)
		{
			if(LawEnforcementRadio[i] == 1) {
				format(string, sizeof(string), "COP MESSAGE: %s(%d) %s",pname,playerid,output);
				Inter_SendClientMessage(i,COLOR_ROYALBLUE,string);
			}
		}
	}
	return 1;
}
