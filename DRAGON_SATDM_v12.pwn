CMD:checkinactive(playerid,params[]){
	#pragma unused params
	if(AccInfo[playerid][Level] >=3) {
		inactivepcheck();
		Inter_SendClientMessage(playerid,COLOR_RED, "Inactive Player Check Complete.");
		return 1;
	}
	else {
		Inter_SendClientMessage(playerid,COLOR_RED, "You Must be at least Admin Level 3 to do that!");
	}
	return 1;
}

timer inactivepcheck[86400000]()
{
	new buf[64], i;
	i = 0;
	while(ffind("/LuxAdmin/Accounts/*.ini", buf, sizeof(buf), i))
	{
		new fname[MAX_STRING];
		new years,months,days;
		new year,month,day; getdate(year, month, day);
		new dyear,dyears,dmonth,dmonths;
		new tyears,tmonths,tdays;
		new OfflineDays;
		new SplitDiv[99][3];

		format(fname,sizeof(fname),"/LuxAdmin/Accounts/%s",buf);
		split(DOF2_GetString(fname,"LastOn"), SplitDiv, '.');
		days = strval(SplitDiv[0]);
		months = strval(SplitDiv[1]);
		years = strval(SplitDiv[2]);

		tdays = day-days;
		dmonth = month*30;
		dyear = year*365;
		dmonths = months*30; tmonths = dmonth-dmonths;
		dyears = years*365; tyears = dyear-dyears;
		OfflineDays = tdays+tmonths+tyears;
		if(OfflineDays >=31 && DOF2_GetInt(fname,"Level") == 0 && DOF2_GetInt(fname,"AccountType") == 0) {
		    new vehicleID1 = DOF2_GetInt(fname,"vowned");
		    new vehicleID2 = DOF2_GetInt(fname,"vowned2");
		    new vehicleID3 = DOF2_GetInt(fname,"vowned3");
		    new vehicleID4 = DOF2_GetInt(fname,"vowned4");
		    new vehicleID5 = DOF2_GetInt(fname,"vowned5");
		    new vehicleID6 = DOF2_GetInt(fname,"vowned6");
		    new vehicleID7 = DOF2_GetInt(fname,"vowned7");
		    new vehicleID8 = DOF2_GetInt(fname,"vowned8");
		    new vehicleID9 = DOF2_GetInt(fname,"vowned9");
		    new vehicleID10 = DOF2_GetInt(fname,"vowned10");
#if defined EXTRA_V_SLOTS
		    new vehicleID11 = DOF2_GetInt(fname,"vowned11");
		    new vehicleID12 = DOF2_GetInt(fname,"vowned12");
		    new vehicleID13 = DOF2_GetInt(fname,"vowned13");
		    new vehicleID14 = DOF2_GetInt(fname,"vowned14");
		    new vehicleID15 = DOF2_GetInt(fname,"vowned15");
#endif
		    new bizID = DOF2_GetInt(fname,"bowned");
		    if(vehicleID1 > 0) ResetOfflinePlayerFileV(fname, vehicleID1);
			if(vehicleID2 > 0) ResetOfflinePlayerFileV(fname, vehicleID2);
			if(vehicleID3 > 0) ResetOfflinePlayerFileV(fname, vehicleID3);
			if(vehicleID4 > 0) ResetOfflinePlayerFileV(fname, vehicleID4);
			if(vehicleID5 > 0) ResetOfflinePlayerFileV(fname, vehicleID5);
		    if(vehicleID6 > 0) ResetOfflinePlayerFileV(fname, vehicleID6);
			if(vehicleID7 > 0) ResetOfflinePlayerFileV(fname, vehicleID7);
			if(vehicleID8 > 0) ResetOfflinePlayerFileV(fname, vehicleID8);
			if(vehicleID9 > 0) ResetOfflinePlayerFileV(fname, vehicleID9);
			if(vehicleID10 > 0) ResetOfflinePlayerFileV(fname, vehicleID10);
#if defined EXTRA_V_SLOTS
		    if(vehicleID11 > 0) ResetOfflinePlayerFileV(fname, vehicleID11);
			if(vehicleID12 > 0) ResetOfflinePlayerFileV(fname, vehicleID12);
			if(vehicleID13 > 0) ResetOfflinePlayerFileV(fname, vehicleID13);
			if(vehicleID14 > 0) ResetOfflinePlayerFileV(fname, vehicleID14);
			if(vehicleID15 > 0) ResetOfflinePlayerFileV(fname, vehicleID15);
#endif
			if(bizID > 0) ResetOfflinePlayerFileB(fname, bizID);

			new str1[256],housenumber;
			housenumber = DOF2_GetInt(fname, "Houseid");
			if(housenumber > -255) {
				DOF2_SetInt(fname, "Houseid", -255);
				if(DOF2_GetInt(fname,"Rentid") > -255) DOF2_SetInt(fname, "Rentid", -255);
				DOF2_SetInt(fname, "money", DOF2_GetInt(fname, "money")+hInfo[housenumber][Sell]);
				DestroyPickup(HousePickup[housenumber]);
				HousePickup[housenumber] = CreatePickup(1273, 23, hInfo[housenumber][iconx], hInfo[housenumber][icony], hInfo[housenumber][iconz]);

				format(str1, sizeof(str1), "SATDM/Houses/houseid%d", housenumber);
				if (!dini_Exists(str1)) dini_Create(str1);
			    dini_Set(str1, "Name", DEFAULT_OWNER);
			    hInfo[housenumber][Locked] = dini_IntSet(str1,"Locked",1);
			    format(hInfo[housenumber][Name],255,DEFAULT_OWNER);

				new hnowner[30];
				format(hnowner,128,"%s",hInfo[housenumber][Name]);
				if(strcmp(hInfo[housenumber][Name],DEFAULT_OWNER,false) == 0) {
			    	format(hnowner,128,"Not Owned");
				}
				UpdateDynamic3DTextLabelText(hTextInfo[housenumber],0x00FF00FF,hnowner);
				DestroyDynamicMapIcon(housemapicon[housenumber]);
				housemapicon[housenumber] = CreateDynamicMapIcon(hInfo[housenumber][iconx], hInfo[housenumber][icony], hInfo[housenumber][iconz],31,c_y);
			}
			DOF2_RemoveFile(fname);
			new string[128];
			format(string,128,"Inactive Player found: %s",buf);
			for(new ipl; ipl<MAX_PLAYERS; ipl++) {
			    if (AccInfo[ipl][Level] >0) {
					Inter_SendClientMessage(ipl, COLOR_GREEN,string);
				}
			}
		}
	}
}

stock setbuy(playerid)
{
	new string[128];
    if(VehicleInfo[GetPlayerVehicleID(playerid)][Temp] == 1) {
    	return Inter_SendClientMessage(playerid, COLOR_ORANGE, "This vehicle is only temporary and some commands may not work!");
	}
    if(AccInfo[playerid][LoggedIn] == 0) {
	    Inter_SendClientMessage(playerid, COLOR_BRIGHTRED, "You must be logged in to use this feature");
	    return 1;
	}
	if(passenger[playerid] == 1) {
		Inter_SendClientMessage(playerid, COLOR_BRIGHTRED, "You must be in the drivers seat to use this feature");
		return 1;
	}
	if(IsPlayerInAnyVehicle(playerid) == 0) {
		Inter_SendClientMessage(playerid, COLOR_BRIGHTRED, "You must be in a vehicle in order to use this feature");
		return 1;
	}
	if(VehicleInfo[GetPlayerVehicleID(playerid)][buybar] == 0) {
		Inter_SendClientMessage(playerid, COLOR_BRIGHTRED, "This vehicle is already set as buyable!");
		return 1;
	}
	if (AccInfo[playerid][Level] >= 10) {
		if(VehicleInfo[GetPlayerVehicleID(playerid)][buybar] == 1) {
			format(string,sizeof(string),"You have set this %s to buyable!", VehicleInfo[GetPlayerVehicleID(playerid)][vname]);
			Inter_SendClientMessage(playerid,COLOR_ORANGE,string);
			VehicleInfo[GetPlayerVehicleID(playerid)][buybar] = 0;
			return 1;
		}
	}
	Inter_SendClientMessage(playerid, COLOR_BRIGHTRED, "You are not a server administrator and cannot use this feature!");
	return 1;
}

