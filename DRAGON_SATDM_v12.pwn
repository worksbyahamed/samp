	if(CloseToBank(playerid) == 999) {
		Inter_SendClientMessage(playerid, COLOR_BRIGHTRED, "You must be in a 24/7 in order to use banking!");
		return 1;
	}
	new buyername[24];
	GetPlayerName(playerid,buyername, 24);
	if(GetPlayerCash(playerid) <= 200*amount-1) {
	   Inter_SendClientMessage(playerid, 0xA9A9A9AA, "|_Weed Purchase Failed_|");
