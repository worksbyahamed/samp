	if(RaceBuilders[playerid] != 0)
	{
		if(BAirrace[b(playerid)] == 0)
		{
			Inter_SendClientMessage(playerid,COLOR_GREEN,"Air race enabled.");
			BAirrace[b(playerid)]=1;
		}
		else
		{
			Inter_SendClientMessage(playerid,COLOR_GREEN,"Air race disabled.");
			BAirrace[b(playerid)]=0;
		}
		return 1;
	}
	if(RaceAdmin == 1 && IsNotAdmin(playerid)) return 1;
	if(RaceActive == 1 || RaceStart == 1) Inter_SendClientMessage(playerid, COLOR_YELLOW, "Race is already in progress!");
	else if(LCurrentCheckpoint == 0) Inter_SendClientMessage(playerid, COLOR_YELLOW, "No race loaded!");
