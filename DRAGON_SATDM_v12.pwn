	if(AccInfo[playerid][LoggedIn] == 1 && AccInfo[playerid][bowner] == 1) {
	    TimedTele(playerid, "Your Business!", BizInfo[AccInfo[playerid][bowned]][xpos],BizInfo[AccInfo[playerid][bowned]][ypos],BizInfo[AccInfo[playerid][bowned]][zpos], BizInfo[AccInfo[playerid][bowned]][inter], GetPlayerVirtualWorld(playerid), 11);
		return 1;
	}
	Inter_SendClientMessage(playerid, COLOR_BRIGHTRED, "You do not own a business yet!");
	return 1;
}
//---------------------------------Admin Sell Biz-------------------------------
CMD:asellbiz(playerid,params[]) {
	#pragma unused params
	new string[128];
	if(AccInfo[playerid][LoggedIn] == 0) {
		Inter_SendClientMessage(playerid, COLOR_BRIGHTRED, "You must be logged in to use this command");
		return 1;
	}
	if(AccInfo[playerid][Jailed] == 1) {
		Inter_SendClientMessage(playerid, COLOR_BRIGHTRED, "You are in jail and cannot use this command");
		return 1;
	}
	if(IsPlayerInAnyVehicle(playerid) == 1) {
		Inter_SendClientMessage(playerid, COLOR_BRIGHTRED, "You must be on foot in order to sell a business!");
		return 1;
	}
	if(ClosestBiz(playerid) >= 999) {
		Inter_SendClientMessage(playerid, COLOR_BRIGHTRED, "You are not close enough to any of the current business icons to use this command");
		return 1;
	}
	if (AccInfo[playerid][Level] >= 5) {
	    if(BizInfo[ClosestBiz(playerid)][bought] == 1)
		{
			format(string,sizeof(string),"This Business (%d) does not have an owner yet!",BizInfo[ClosestBiz(playerid)][bname]);
			Inter_SendClientMessage(playerid,COLOR_BRIGHTRED,string);
			return 1;
		}
		if(DOF2_FileExists(DOF2_File(BizInfo[ClosestBiz(playerid)][owner]))) {
			tempid[playerid] = IsPlayerNameOnline(BizInfo[ClosestBiz(playerid)][owner]);
			if(tempid[playerid] >= 0 || tempid[playerid] <= MAX_PLAYERS) {
				strmid(BizInfo[ClosestBiz(playerid)][owner], DEFAULT_OWNER, 0, strlen(DEFAULT_OWNER), 255);
				BizInfo[ClosestBiz(playerid)][bought] = 0;
				BizInfo[ClosestBiz(playerid)][cashbox] = 0;

				new cbmon = BizInfo[ClosestBiz(playerid)][cashbox];
				new bmon = AccInfo[playerid][bank];
				AccInfo[tempid[playerid]][bank] = cbmon+bmon;
				AccInfo[tempid[playerid]][bowned] = 0;
				AccInfo[tempid[playerid]][bowner] = 0;

				format(string, sizeof(string), "You just sold %s's Business (%s).", PlayerName2(tempid[playerid]), BizInfo[ClosestBiz(playerid)][bname]);
				Inter_SendClientMessage(playerid, COLOR_ORANGE, string);
				format(string, sizeof(string), "Admin (%s) has just sold your Business (%s)! Your cashbox money was sent to your personal bank account", PlayerName2(playerid),BizInfo[ClosestBiz(playerid)][bname]);
				Inter_SendClientMessage(tempid[playerid], COLOR_ORANGE, string);
				new bizowner[30];
				format(bizowner,128,"%s",BizInfo[ClosestBiz(playerid)][owner]);
				if(strcmp(BizInfo[ClosestBiz(playerid)][owner],DEFAULT_OWNER,false) == 0) {
			    	format(bizowner,128,"Not Owned");
				}
				UpdateDynamic3DTextLabelText(bizTextInfo[ClosestBiz(playerid)],0xFF0000FF,bizowner);
				new bId = ClosestBiz(playerid);
				DestroyDynamicMapIcon(BizInfo[bId][ico]);
				BizInfo[bId][ico] = CreateDynamicMapIcon(BizInfo[bId][xpos],BizInfo[bId][ypos],BizInfo[bId][zpos],31,250);
				return 1;
			}
			else {
				ResetOfflinePlayerFileB(PlayerName2(tempid[playerid]), ClosestBiz(playerid));
				format(string, sizeof(string), "You just sold %s's Business (%s).",PlayerName2(tempid[playerid]), BizInfo[ClosestBiz(playerid)][bname]);
				Inter_SendClientMessage(playerid, COLOR_ORANGE, string);
				return 1;
			}
		}
		else {
			format(string, sizeof(string), "You just sold %s's Business (%s). The player file was not found and cannot be altered.",PlayerName2(tempid[playerid]), BizInfo[ClosestBiz(playerid)][bname]);
			Inter_SendClientMessage(playerid, COLOR_GREEN, string);
			BizInfo[ClosestBiz(playerid)][bought] = 0;
			BizInfo[ClosestBiz(playerid)][cashbox] = 0;
			strmid(BizInfo[ClosestBiz(playerid)][owner], DEFAULT_OWNER, 0, strlen(DEFAULT_OWNER), 255);
			new bizowner[30];
			format(bizowner,128,"%s",BizInfo[ClosestBiz(playerid)][owner]);
			if(strcmp(BizInfo[ClosestBiz(playerid)][owner],DEFAULT_OWNER,false) == 0) {
		    	format(bizowner,128,"Not Owned");
			}
			UpdateDynamic3DTextLabelText(bizTextInfo[ClosestBiz(playerid)],0xFF0000FF,bizowner);
			new bId = ClosestBiz(playerid);
			DestroyDynamicMapIcon(BizInfo[bId][ico]);
			BizInfo[bId][ico] = CreateDynamicMapIcon(BizInfo[bId][xpos],BizInfo[bId][ypos],BizInfo[bId][zpos],31,250);
			return 1;
		}
	}
	Inter_SendClientMessage(playerid, COLOR_BRIGHTRED, "You are not a server administrator and cannot use this command!");
	return 1;
}
//-----------------------------------Buy Biz------------------------------------
CMD:buybiz(playerid,params[]) {
	#pragma unused params
	new string[128];
	if(AccInfo[playerid][LoggedIn] == 0) {
		Inter_SendClientMessage(playerid, COLOR_BRIGHTRED, "You must be logged in to use this command");
		return 1;
	}
	if(AccInfo[playerid][Jailed] == 1) {
		Inter_SendClientMessage(playerid, COLOR_BRIGHTRED, "You are in jail and cannot use this command");
		return 1;
	}
	if(IsPlayerInAnyVehicle(playerid) == 1) {
		Inter_SendClientMessage(playerid, COLOR_BRIGHTRED, "You must be on foot in order to purchase a business!");
		return 1;
	}
	if(ClosestBiz(playerid) == 999) {
		Inter_SendClientMessage(playerid, COLOR_BRIGHTRED, "You are not close enough to any of the current business icons to use this command");
		return 1;
	}
	if (strcmp(BizInfo[ClosestBiz(playerid)][owner],PlayerName2(playerid),false) == 0) {
		format(string, sizeof(string), "You already own this Business (%s)", BizInfo[ClosestBiz(playerid)][bname]);
		Inter_SendClientMessage(playerid, COLOR_BRIGHTRED, string);
		return 1;
	}
	if(BizInfo[ClosestBiz(playerid)][bought] == 1) {
		format(string, sizeof(string), "This Business (%s) is owned by %s, and is not for sale!", BizInfo[ClosestBiz(playerid)][bname], BizInfo[ClosestBiz(playerid)][owner]);
		Inter_SendClientMessage(playerid, COLOR_BRIGHTRED, string);
		return 1;
	}
	if(AccInfo[playerid][bowner] == 1) {
		Inter_SendClientMessage(playerid, COLOR_BRIGHTRED, "You can only own ONE Business at a time! You must sell your other Business first!");
 		return 1;
	}
	new cash[MAX_PLAYERS];
	cash[playerid] = GetPlayerCash(playerid);
	if(cash[playerid] >= BizInfo[ClosestBiz(playerid)][cost]) {
		new stringa[128];
		strmid(BizInfo[ClosestBiz(playerid)][owner], PlayerName2(playerid), 0, strlen(PlayerName2(playerid)), 255);
		BizInfo[ClosestBiz(playerid)][bought] = 1;
		AccInfo[playerid][bowner] = 1;
		AccInfo[playerid][bowned] = ClosestBiz(playerid);
		GivePlayerCash(playerid, -BizInfo[ClosestBiz(playerid)][cost]);
		format(stringa, sizeof(stringa), "You just bought this Business (%s) for $%d. Your Business will earn you $%d/hr", BizInfo[ClosestBiz(playerid)][bname], BizInfo[ClosestBiz(playerid)][cost],BizInfo[ClosestBiz(playerid)][profit]*2);
		Inter_SendClientMessage(playerid, COLOR_GREEN, stringa);

		new bizowner[30];
		format(bizowner,128,"%s",BizInfo[ClosestBiz(playerid)][owner]);
		if(strcmp(BizInfo[ClosestBiz(playerid)][owner],DEFAULT_OWNER,false) == 0) {
	    	format(bizowner,128,"Not Owned");
		}
		UpdateDynamic3DTextLabelText(bizTextInfo[ClosestBiz(playerid)],0xFF0000FF,bizowner);
		new bId = ClosestBiz(playerid);
		DestroyDynamicMapIcon(BizInfo[bId][ico]);
		BizInfo[bId][ico] = CreateDynamicMapIcon(BizInfo[bId][xpos],BizInfo[bId][ypos],BizInfo[bId][zpos],32,250);
		SavePlayerStats(playerid);
		SaveBusinesses();
		return 1;
	}
	if(cash[playerid] < BizInfo[ClosestBiz(playerid)][cost]) {
		format(string, sizeof(string), "You do not have $%d and cannot afford this Business (%s)!", BizInfo[ClosestBiz(playerid)][cost],BizInfo[ClosestBiz(playerid)][bname]);
		Inter_SendClientMessage(playerid, COLOR_BRIGHTRED, string);
		return 1;
	}
	return 1;
}
